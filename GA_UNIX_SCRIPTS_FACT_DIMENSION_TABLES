CREATE TABLE ga_products_dim(
PROD_DIM_KEY	Integer,
PROD_CODE	varchar(8),
PROD_NAME	varchar(46),
PROD_DESC	varchar(51),
PROD_SUBCATEGORY	varchar(20),
PROD_SUBCATEGORY_DESC	varchar(20),
PROD_CATEGORY	varchar(27),
PROD_CATEGORY_DESC	varchar(27),
PROD_WEIGHT_CLASS	Integer,
PROD_UNIT_OF_MEASURE	char(1),
PROD_PACK_SIZE	char(1),
PROD_STATUS	varchar(6),
PROD_LIST_PRICE	decimal(6,2),
PROD_MIN_PRICE	decimal(6,2),
PROD_TOTAL	decimal(6,2),
INSERT_DT	date,
LAST_UPDATE_DT	date,
PROD_VALID	char(1),
VERSION	varchar(8),
DW_LOAD_DATE	date,
DW_UPDATE_DATE	date,
DELETE_STATUS	char(1),
PRIMARY KEY(PROD_DIM_KEY));

CREATE TABLE ga_promotions_dim(
Promo_Dim_Key	Integer,
PROMO_CODE	Varchar(9),
PROMO_NAME	Varchar(27),
PROMO_SUBCATEGORY	Varchar(27),
PROMO_CATEGORY	Varchar(9),
PROMO_COST	Integer,
PROMO_BEGIN_DATE	Date,
PROMO_END_DATE	Date,
PROMO_TOTAL	Varchar(15),
INSERT_DT	Date,
LAST_UPDATE_DT	Date,
VERSION	Varchar(8),
DW_LOAD_DATE	Date,
DW_UPDATE_DATE	Date,
DELETE_STATUS	char(1)
PRIMARY KEY(PROD_DIM_KEY));

CREATE TABLE ga_customers_dim(
Cust_Dim_Key	Integer,
CUST_CODE	Varchar(10),
CUST_FIRST_NAME	Varchar(30),
CUST_LAST_NAME	Varchar(30),
CUST_GENDER	Char(1),
CUST_YEAR_OF_BIRTH	integer,
CUST_MARITAL_STATUS	Varchar(7),
CUST_STREET_ADDRESS	Varchar(32),
CUST_POSTAL_CODE	integer,
CUST_CITY	Varchar(22),
CUST_STATE_PROVINCE	Varchar(32),
COUNTRY_CODE	Varchar(11),
COUNTRY_ISO_CODE	Char(2),
COUNTRY_NAME	Varchar(24),
COUNTRY_SUBREGION	Varchar(16),
COUNTRY_REGION	Varchar(11),
COUNTRY_TOTAL	Varchar(11),
CUST_MAIN_PHONE_NUMBER	Integer,
CUST_PHONE_VALIDITY	Char(1),
CUST_INCOME_LEVEL	Varchar(20),
CUST_CREDIT_LIMIT	Integer,
CUST_EMAIL	Varchar(23),
CUST_EMAIL_VALIDITY	Char(1),
INSERT_DATE	Date,
LAST_UPDATE_DT	Date,
CUST_VALID	char(1),
VERSION	Varchar(8),
DW_LOAD_DATE	Date,
DW_UPDATE_DATE	Date,
DELETE_STATUS	char(1)
PRIMARY KEY(CUST_DIM_KEY));

CREATE TABLE ga_channels_dim(
Channels_Dim_Key	Integer,
CHANNEL_CODE	Varchar(6),
CHANNEL_DESC	Char(12),
CHANNEL_CLASS	Char(8),
INSERT_DT	Date,
LAST_UPDATE_DT	Date,
VERSION	Varchar(8),
DW_LOAD_DATE	Date,
DW_UPDATE_DATE	Date,
DELETE_STATUS	char(1),
PRIMARY KEY(CHANNELS_DIM_KEY));

CREATE TABLE ga_sales_fact(
SALES_FACT_KEY	Integer,
ORDER_NUMBER	varchar(10),
PROD_CODE	varchar(10),
Prod_Dim_Key	Integer(10),
CUST_CODE	varchar(10),
Cust_Dim_Key	Integer,
SALE_DATE	Date,
CHANNEL_CODE	varchar(10),
Channel_Dim_Key	Integer,
PROMO_CODE	varchar(10),
Promo_Dim_Key	Integer,
QUANTITY_SOLD	Integer,
AMOUNT_SOLD	Integer,
INSERT_DT	Date,
LAST_UPDATE_DT	Date,
DW_LOAD_DATE	Date,
DW_UPDATE_DATE	Date,
DELETE_STATUS	char(1),
PRIMARY KEY(SALES_FACT_KEY),
FOREIGN KEY(Prod_Dim_Key) references ga_products_dim(PROD_CODE),
FOREIGN KEY(Cust_Dim_Key) references ga_customers_dim(CUST_CODE),
FOREIGN KEY(Promo_Dim_Key) references ga_promotions_dim(PROMO_CODE),
FOREIGN KEY(Channel_Dim_Key) references ga_channels_dim(CHANNEL_CODE));